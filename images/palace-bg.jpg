<svg xmlns="http://www.w3.org/2000/svg" width="750" height="1334" viewBox="0 0 750 1334">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#f9e8d4" />
      <stop offset="100%" stop-color="#e6c9a8" />
    </linearGradient>
    <pattern id="pattern-bg" width="20" height="20" patternUnits="userSpaceOnUse">
      <path d="M0 0h20v20H0z" fill="url(#bg-gradient)" />
      <path d="M0 10h20M10 0v20" stroke="#d4b08c" stroke-width="0.5" opacity="0.3" />
    </pattern>
  </defs>
  
  <!-- 背景 -->
  <rect width="750" height="1334" fill="url(#pattern-bg)" />
  
  <!-- 宫殿轮廓 -->
  <g transform="translate(150, 200)" stroke="#9b4e20" stroke-width="2" fill="none">
    <!-- 宫殿屋顶 -->
    <path d="M0,200 L225,50 L450,200" fill="#c17946" fill-opacity="0.3" />
    
    <!-- 宫殿主体 -->
    <rect x="50" y="200" width="350" height="250" fill="#d4b08c" fill-opacity="0.3" />
    
    <!-- 宫殿柱子 -->
    <rect x="75" y="200" width="20" height="250" fill="#9b4e20" fill-opacity="0.5" />
    <rect x="175" y="200" width="20" height="250" fill="#9b4e20" fill-opacity="0.5" />
    <rect x="275" y="200" width="20" height="250" fill="#9b4e20" fill-opacity="0.5" />
    <rect x="375" y="200" width="20" height="250" fill="#9b4e20" fill-opacity="0.5" />
    
    <!-- 宫殿门 -->
    <rect x="175" y="300" width="100" height="150" rx="10" fill="#9b4e20" fill-opacity="0.4" />
    
    <!-- 宫殿窗户 -->
    <rect x="75" y="250" width="50" height="80" rx="5" fill="#f9e8d4" fill-opacity="0.5" />
    <rect x="325" y="250" width="50" height="80" rx="5" fill="#f9e8d4" fill-opacity="0.5" />
  </g>
  
  <!-- 装饰元素 -->
  <g opacity="0.2">
    <!-- 云纹 -->
    <path d="M50,100 Q75,80 100,100 Q125,120 150,100 Q175,80 200,100" stroke="#9b4e20" stroke-width="2" fill="none" />
    <path d="M500,150 Q525,130 550,150 Q575,170 600,150 Q625,130 650,150" stroke="#9b4e20" stroke-width="2" fill="none" />
    
    <!-- 花纹 -->
    <circle cx="100" cy="500" r="30" fill="#9b4e20" fill-opacity="0.2" />
    <circle cx="650" cy="600" r="30" fill="#9b4e20" fill-opacity="0.2" />
    <circle cx="200" cy="800" r="40" fill="#9b4e20" fill-opacity="0.2" />
    <circle cx="550" cy="900" r="40" fill="#9b4e20" fill-opacity="0.2" />
  </g>
</svg>